{
  "name": "solomon",
  "version": "0.0.2",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "build": "ng build --prod",
    "build-i18n:en": "ng build --output-path=dist/en --prod --bh /en/ --i18n-file=i18n/messages.en.xlf --i18n-format=xlf --locale=en",
    "build-i18n:zh-Hans": "ng build --output-path=dist/zh-Hans --prod --bh /zh-Hans/ --i18n-file=i18n/messages.zh-Hans.xlf --i18n-format=xlf --locale=zh-Hans",
    "build-i18n:zh-Hant": "ng build --output-path=dist/zh-Hant --prod --bh /zh-Hant/ --i18n-file=i18n/messages.zh-Hant.xlf --i18n-format=xlf --locale=zh-Hant",
    "build-i18n": "npm run build-i18n:en && npm run build-i18n:zh-Hans && npm run build-i18n:zh-Hant",
    "render": "tsc render.ts --skipLibCheck && node render.js"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.3.1",
    "@angular/compiler": "^2.3.1",
    "@angular/core": "^2.3.1",
    "@angular/flex-layout": "^2.0.0-rc.1",
    "@angular/forms": "^2.3.1",
    "@angular/http": "^2.3.1",
    "@angular/material": "^2.0.0-beta.2",
    "@angular/platform-browser": "^2.3.1",
    "@angular/platform-browser-dynamic": "^2.3.1",
    "@angular/platform-server": "^2.4.6",
    "@angular/router": "^3.3.1",
    "@types/cheerio": "^0.17.31",
    "@types/firebase": "^2.4.31",
    "@types/marked": "^0.0.28",
    "angularfire2": "^2.0.0-beta.8",
    "cheerio": "^0.22.0",
    "core-js": "^2.4.1",
    "firebase": "^3.6.9",
    "marked": "^0.3.6",
    "rxjs": "^5.0.1",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@angular/cli": "^1.0.0-rc.1",
    "@angular/compiler-cli": "^2.3.1",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.42",
    "codelyzer": "~2.0.0-beta.1",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "protractor": "~4.0.13",
    "ts-node": "1.2.1",
    "tslint": "^4.3.0",
    "typescript": "~2.0.3"
  }
}
